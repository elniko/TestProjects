				<envelope>
					<XbrlDelivery>
						<XbrlDeclarationReport xmlns="http://www.onegate.eu/2010-01-01">
							<Administration creationTime="{*date*}">
								<From declarerType="FI_ID">{*entity*}</From>
								<To>EBA</To>
								<Domain>{domain}</Domain>
								<Response feedback="{*feedback*}">
									<Email>{*mail*}</Email>
									<Language>{*lang*}</Language>
								</Response>
